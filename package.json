{
  "name": "chute_kun",
  "version": "1.0.0",
  "description": "TaskChute methodology implementation with Todoist API integration",
  "main": "dist/index.js",
  "bin": {
    "chute": "dist/index.js"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "cli": "ts-node src/index.ts",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.{ts,js,json}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/annenpolka/chute_kun.git"
  },
  "keywords": [
    "taskchute",
    "todoist",
    "task-management",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/annenpolka/chute_kun/issues"
  },
  "homepage": "https://github.com/annenpolka/chute_kun#readme",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^22.13.8",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@doist/todoist-api-typescript": "^4.0.0-alpha.3"
  }
